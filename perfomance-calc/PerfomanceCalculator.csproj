  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="OsuApi.Core" Version="0.0.1" />
    <PackageReference Include="ppy.osu.Game" Version="2025.420.0" />
    <PackageReference Include="ppy.osu.Game.Rulesets.Osu" Version="2025.420.0" />
  </ItemGroup>

</Project>
